Numbers=np.array([4,3,9,3,1])
swap2=0
for i in range(1,len(Numbers)):
    temp=Numbers[i] 
    index=i 
    swap2=swap2+1
    print(Numbers) 
    while(index !=0 and Numbers[index-1]>temp):
        Numbers[index]=Numbers[index-1]#
        index-=1
        
        
    
    Numbers[index]=temp
    
print(Numbers)
print("The tottal Swapping are " ,swap2)
